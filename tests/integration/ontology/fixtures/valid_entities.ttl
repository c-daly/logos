@prefix logos: <http://logos.ai/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ========== Flexible Ontology Test Data ==========
# All nodes use logos:Node with type/ancestors properties
# See docs/plans/2025-12-30-flexible-ontology-design.md

# ========== Bootstrap Types ==========

logos:type-type_definition a logos:Node ;
    logos:uuid "c8d4bdc0-a619-5328-a410-a5fce1cec3c5" ;
    logos:name "type_definition" ;
    logos:is_type_definition true ;
    logos:type "type_definition" ;
    logos:ancestors () .

logos:type-thing a logos:Node ;
    logos:uuid "a1234567-89ab-5cde-f012-3456789abcde" ;
    logos:name "thing" ;
    logos:is_type_definition true ;
    logos:type "thing" ;
    logos:ancestors () .

logos:type-concept a logos:Node ;
    logos:uuid "f8b89a6c-9c3e-5e4d-b2f1-83a4d7e4c5f2" ;
    logos:name "concept" ;
    logos:is_type_definition true ;
    logos:type "concept" ;
    logos:ancestors () .

# ========== Custom Type Definitions ==========

# entity IS_A thing
logos:type-entity a logos:Node ;
    logos:uuid "e003e45c-50bd-5e4b-85db-883756ecfcf7" ;
    logos:name "entity" ;
    logos:is_type_definition true ;
    logos:type "entity" ;
    logos:ancestors ("thing") .

# state IS_A concept
logos:type-state a logos:Node ;
    logos:uuid "2dd3d0d8-569f-5132-b4a1-7ec30dc2d92b" ;
    logos:name "state" ;
    logos:is_type_definition true ;
    logos:type "state" ;
    logos:ancestors ("concept") .

# process IS_A concept
logos:type-process a logos:Node ;
    logos:uuid "a62326cc-bd8b-5378-a428-9600eb3bafe8" ;
    logos:name "process" ;
    logos:is_type_definition true ;
    logos:type "process" ;
    logos:ancestors ("concept") .

# robot_state IS_A state
logos:type-robot_state a logos:Node ;
    logos:uuid "6f7a8b9c-0d1e-5f2a-3b4c-5d6e7f8a9b0c" ;
    logos:name "robot_state" ;
    logos:is_type_definition true ;
    logos:type "robot_state" ;
    logos:ancestors ("state" "concept") .

# ========== Instances ==========

# Valid entity instance
logos:instance-entity-001 a logos:Node ;
    logos:uuid "instance-entity-001" ;
    logos:name "ValidTestEntity" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") ;
    logos:description "A valid test entity" .

# Valid concept instance
logos:instance-concept-001 a logos:Node ;
    logos:uuid "instance-concept-001" ;
    logos:name "ValidConcept" ;
    logos:is_type_definition false ;
    logos:type "concept" ;
    logos:ancestors ("concept") ;
    logos:description "A valid test concept" .

# Valid state instance
logos:instance-state-001 a logos:Node ;
    logos:uuid "instance-state-001" ;
    logos:name "ValidState" ;
    logos:is_type_definition false ;
    logos:type "state" ;
    logos:ancestors ("state" "concept") ;
    logos:timestamp "2025-11-17T00:00:00Z"^^xsd:dateTime .

# Valid process instance
logos:instance-process-001 a logos:Node ;
    logos:uuid "instance-process-001" ;
    logos:name "ValidProcess" ;
    logos:is_type_definition false ;
    logos:type "process" ;
    logos:ancestors ("process" "concept") ;
    logos:start_time "2025-11-17T00:00:00Z"^^xsd:dateTime ;
    logos:duration_ms 1000 .

# ========== Instances with Domain Properties ==========

# Entity with spatial properties
logos:instance-spatial-001 a logos:Node ;
    logos:uuid "instance-spatial-001" ;
    logos:name "SpatialEntity" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") ;
    logos:width 0.5 ;
    logos:height 0.3 ;
    logos:depth 0.2 ;
    logos:mass 1.5 .

# State with position properties
logos:instance-state-spatial a logos:Node ;
    logos:uuid "instance-state-spatial" ;
    logos:name "SpatialState" ;
    logos:is_type_definition false ;
    logos:type "robot_state" ;
    logos:ancestors ("robot_state" "state" "concept") ;
    logos:timestamp "2025-11-17T01:00:00Z"^^xsd:dateTime ;
    logos:position_x 1.5 ;
    logos:position_y 0.0 ;
    logos:position_z 0.5 .

# Entity with joint properties
logos:instance-joint-001 a logos:Node ;
    logos:uuid "instance-joint-001" ;
    logos:name "RevoluteJoint" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") ;
    logos:joint_type "revolute" ;
    logos:min_angle -3.14 ;
    logos:max_angle 3.14 .

# ========== Relationships ==========

logos:instance-parent a logos:Node ;
    logos:uuid "instance-parent" ;
    logos:name "ParentEntity" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") .

logos:instance-child a logos:Node ;
    logos:uuid "instance-child" ;
    logos:name "ChildEntity" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") ;
    logos:PART_OF logos:instance-parent .

logos:instance-with-state a logos:Node ;
    logos:uuid "instance-with-state" ;
    logos:name "EntityWithState" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") ;
    logos:HAS_STATE logos:instance-state-001 .
