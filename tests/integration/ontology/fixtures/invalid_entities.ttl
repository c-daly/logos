@prefix logos: <http://logos.ai/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ========== Invalid Flexible Ontology Test Data ==========
# Nodes that violate SHACL NodeShape constraints
# See docs/plans/2025-12-30-flexible-ontology-design.md

# ========== Missing Required Properties ==========

# Invalid - missing uuid
logos:node-missing-uuid a logos:Node ;
    logos:name "MissingUUID" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") .

# Invalid - missing name
logos:node-missing-name a logos:Node ;
    logos:uuid "node-missing-name" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") .

# Invalid - missing is_type_definition
logos:node-missing-is-type-def a logos:Node ;
    logos:uuid "node-missing-is-type-def" ;
    logos:name "MissingIsTypeDef" ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") .

# Invalid - missing type
logos:node-missing-type a logos:Node ;
    logos:uuid "node-missing-type" ;
    logos:name "MissingType" ;
    logos:is_type_definition false ;
    logos:ancestors ("entity" "thing") .

# Invalid - missing ancestors
logos:node-missing-ancestors a logos:Node ;
    logos:uuid "node-missing-ancestors" ;
    logos:name "MissingAncestors" ;
    logos:is_type_definition false ;
    logos:type "entity" .

# ========== Multiple Missing Properties ==========

# Invalid - missing multiple required properties
logos:node-multiple-missing a logos:Node ;
    logos:name "MultipleMissing" .

# ========== Invalid IS_A Relationship ==========

# IS_A target should be a Node (this tests relationship constraint)
logos:node-invalid-isa a logos:Node ;
    logos:uuid "node-invalid-isa" ;
    logos:name "InvalidIsA" ;
    logos:is_type_definition false ;
    logos:type "entity" ;
    logos:ancestors ("entity" "thing") ;
    logos:IS_A "not-a-node" .
